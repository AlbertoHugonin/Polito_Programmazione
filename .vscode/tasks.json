{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Normale",
			"command": "/usr/bin/gcc-7",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}",
				"-I${workspaceFolder}/Algoritmi",
				"-I${workspaceFolder}/Algoritmi/Calcolo Combinatorio",
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"windows": {
				"command": "C:\\TDM-GCC-64\\bin\\gcc.exe"
			  },
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "shell",
			"label": "Giochi di Ruolo",
			"command": "/usr/bin/gcc-7",
			"args": [
				"-g",
				"${file}","${workspaceFolder}/Laboratori/LAB-9/Giochi di Ruolo Modulare/inventario.c", "${workspaceFolder}/Laboratori/LAB-9/Giochi di Ruolo Modulare/personaggi.c",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}",
				"-I${workspaceFolder}/Algoritmi",
				"-I${workspaceFolder}/Algoritmi/Calcolo Combinatorio",
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"windows": {
				"command": "C:\\TDM-GCC-64\\bin\\gcc.exe"
			  },
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}